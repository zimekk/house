{
  "name": "@dev/tools",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./components/*": [
      "./src/components/*/index.tsx"
    ],
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "files": [
    "src"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "peerDependencies": {
    "next": "~15.5.0",
    "react": "^17.0.0 || ^18.0.0-0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0-0 || ^19.0.0"
  },
  "dependencies": {
    "styled-components": "^6.1.14"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/styled-components": "^5.1.26",
    "eslint": "^9.35.0",
    "typescript": "^5.7.3"
  }
}
